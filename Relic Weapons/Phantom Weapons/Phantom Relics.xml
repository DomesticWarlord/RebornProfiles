    <CodeChunk Type="Definition" Name="Logger">
      <![CDATA[
        public static readonly LlamaLibrary.Logging.LLogger Log = new("Phantom Relics", Colors.DeepPink);

        public static async Task<bool> ChangeJobBackToAccepted(int questId)
        {
            var QuestJob = QuestLogManager.GetQuestById(questId).AcceptedClass.ToString();
            var gearSets = GearsetManager.GearSets.Where(i => i.InUse);
            var foundJob = Enum.TryParse(QuestJob.Trim(), true, out ClassJobType newjob);

            if (Core.Me.CurrentJob == newjob)
            {
                return true;
            }

            Log.Information("Started");
            Log.Information($"Found job: {foundJob} Job:{newjob}");
            if (foundJob && gearSets.Any(gs => gs.Class == newjob))
            {
                Log.Information($"Found GearSet");
                gearSets.First(gs => gs.Class == newjob).Activate();

                await Coroutine.Wait(3000, () => SelectYesno.IsOpen);
                if (SelectYesno.IsOpen)
                {
                    SelectYesno.Yes();
                    await Coroutine.Sleep(3000);
                }

                // await Coroutine.Sleep(1000);
            }

            return true;

        }

    ]]>
