						await Coroutine.Wait(5000, () => Core.Me.CurrentTarget == npcId);
        }

#if !RB_CN
        ChatManager.SendChat("/doubt");
#endif
#if RB_CN
        ChatManager.SendChat("/doubt");
#endif
        await Coroutine.Wait(10000, () => Talk.DialogOpen);
        await Buddy.Coroutines.Coroutine.Sleep(500);
        while (QuestLogManager.GetQuestById(70916).Step == currentQuestStep)
        {
            Talk.Next();
